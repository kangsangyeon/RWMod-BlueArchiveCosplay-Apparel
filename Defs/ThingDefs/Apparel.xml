	<ThingDef ParentName="ApparelMakeableBase">
		<defName>Makoto_uniform</defName>
		<label>Makoto uniform</label>
		<description>Makoto uniform</description>
		<graphicData>
			<texPath>Things/Apparel/makoto</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<tradeability>Sellable</tradeability>
		<costList>
			<Cloth>140</Cloth>
			<DevilstrandCloth>75</DevilstrandCloth>
		</costList>
		<thingCategories>
			<li>Apparel</li>
		</thingCategories>
		<statBases>
			<MaxHitPoints>500</MaxHitPoints>
			<WorkToMake>8000</WorkToMake>
			<Mass>1.2</Mass>
			<ArmorRating_Blunt>0.15</ArmorRating_Blunt>
			<ArmorRating_Sharp>0.45</ArmorRating_Sharp>
			<ArmorRating_Heat>0.5</ArmorRating_Heat>
			<Insulation_Cold>10</Insulation_Cold>
			<Insulation_Heat>40</Insulation_Heat>
			<Beauty>15</Beauty>
			<EquipDelay>2</EquipDelay>
		</statBases>
		<equippedStatOffsets>
			<MoveSpeed>0.5</MoveSpeed>
			<SocialImpact>0.3</SocialImpact>
		</equippedStatOffsets>
		<apparel>
			<bodyPartGroups>
